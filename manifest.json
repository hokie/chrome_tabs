{
  "manifest_version": 2,

  "name": "Tabsplode",
  "description": "This extension opens lists of websites in seperate tabs",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "storage"
  ]
}
